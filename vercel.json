{
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/favicon.png",
            "dest": "/favicon.png"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}